<html lang="en">
  <head>
    {{> head }}
    <title>Amrita Trees | Game</title>
    <link rel="stylesheet" href="/css/styles.css" />
  </head>
  <script src="/js/app.js" type="module"></script>
  <link rel="stylesheet" href="/leaflet/leaflet.css"/>
  <script src="/leaflet/leaflet.js"></script>

<body class="w-screen h-screen relative bg-slate-800 overflow-hidden">
  <div id="TailwindRequiredStyles" class="hidden translate-x-[300px]">

  </div>

  {{> locationPermission }}

  <div id="mapWrapper" class="w-screen relative transition-all">
    <div id="map" class="w-screen" style="height: 100vh;"></div>
    <div id="mapOverlay" style="z-index:999;" class="w-screen h-screen absolute top-0 left-0 bg-black opacity-20 hidden">

    </div>
  </div>

  {{> gameNavbar}}

  {{> sideBar }}
  {{> questionPopup }}
</body>
<script src="/js/mapScript.js" type="module"></script>
<script src="/js/gameScript.js" type="module"></script>
</html>